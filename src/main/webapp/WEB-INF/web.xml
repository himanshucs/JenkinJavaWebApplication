<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Build</servlet-name>
  	<display-name>Build</display-name>
  	<description></description>
  	<servlet-class>com.JenkinJavaWebApplication.Controller.Build</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BuildDef</servlet-name>
  	<display-name>BuildDef</display-name>
  	<description></description>
  	<servlet-class>com.JenkinJavaWebApplication.Controller.BuildDef</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Build</servlet-name>
  	<url-pattern>/Build</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BuildDef</servlet-name>
  	<url-pattern>/BuildDef</url-pattern>
  </servlet-mapping>
</web-app>
